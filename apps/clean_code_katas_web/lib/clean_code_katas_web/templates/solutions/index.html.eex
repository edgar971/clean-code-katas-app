<section class="hero is-primary">
  <div class="hero-body">
    <div class="container">
      <h1 class="title">
       Solution to: <%= @solution.challenge.title %>
      </h1>
    </div>
  </div>
</section>
<section class="section">
  <div class="container">
    <div class="content">
      <h1>Coder: <%= @solution.user.name %> </h1>
      <p> <%=@solution.description%> </p>
      <pre>
          <%= @solution.code  %>
      </pre>
    </div>
  </div>
</section>

